{
  "version": "0.2",
  "language": "en",
  "words": [
    "octoparse",
    "pytest",
    "pyproject",
    "ruff",
    "dotenv",
    "pycache",
    "venv",
    "sdist",
    "hatchling",
    "pycodestyle",
    "pyflakes",
    "isort",
    "precommit",
    "pytest"
  ],
  "ignorePaths": [
    "node_modules",
    "venv",
    ".git",
    ".venv",
    "*.pyc",
    "dist",
    "build",
    "__pycache__",
    "*.egg-info"
  ],
  "allowCompoundWords": true,
  "patterns": [
    {
      "name": "python_vars",
      "pattern": "/[a-z][a-z0-9_]*/"
    }
  ],
  "ignoreRegExpList": ["python_vars"]
}
